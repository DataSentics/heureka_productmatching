{
  "version": 1,
  "disable_existing_loggers": false,
  "loggers": {
    "": {
      "level": "INFO",
      "handlers": [
        "stdout"
      ]
    },
    "buttstrap": {
      "level": "WARNING"
    }
  },
  "handlers": {
    "stdout": {
      "class": "logging.StreamHandler",
      "level": "WARNING",
      "formatter": "standard"
    }
  },
  "formatters": {
    "standard": {
      "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
    }
  }
}
